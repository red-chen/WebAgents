# WebAgents 配置文件示例
# 复制此文件为 .env 并填入实际配置值

# 基础配置
DEBUG=false
LOG_LEVEL=INFO
CACHE_DIR=.cache
LOG_DIR=logs

# 网络配置
REQUEST_TIMEOUT=30
MAX_RETRIES=3
RETRY_DELAY=1
USER_AGENT=WebAgents/1.0.0

# 缓存配置
CACHE_TTL=3600
ENABLE_FILE_CACHE=true
ENABLE_MEMORY_CACHE=true

# 频率限制配置
RATE_LIMIT_STRATEGY=token_bucket
DEFAULT_RATE_LIMIT=100
DEFAULT_RATE_WINDOW=3600

# Google News 配置
GOOGLE_NEWS_BASE_URL=https://news.google.com
GOOGLE_NEWS_RSS_URL=https://news.google.com/rss
GOOGLE_NEWS_RATE_LIMIT=60
GOOGLE_NEWS_RATE_WINDOW=60

# X (Twitter) 配置
TWITTER_API_KEY=your_twitter_api_key
TWITTER_API_SECRET=your_twitter_api_secret
TWITTER_ACCESS_TOKEN=your_twitter_access_token
TWITTER_ACCESS_TOKEN_SECRET=your_twitter_access_token_secret
TWITTER_BEARER_TOKEN=your_twitter_bearer_token
TWITTER_BASE_URL=https://api.twitter.com/2
TWITTER_RATE_LIMIT=300
TWITTER_RATE_WINDOW=900

# Reddit 配置
REDDIT_CLIENT_ID=your_reddit_client_id
REDDIT_CLIENT_SECRET=your_reddit_client_secret
REDDIT_USER_AGENT=WebAgents/1.0.0
REDDIT_USERNAME=your_reddit_username
REDDIT_PASSWORD=your_reddit_password
REDDIT_BASE_URL=https://www.reddit.com
REDDIT_RATE_LIMIT=60
REDDIT_RATE_WINDOW=60

# 金十数据配置
JIN10_BASE_URL=https://www.jin10.com
JIN10_API_URL=https://api.jin10.com
JIN10_RATE_LIMIT=120
JIN10_RATE_WINDOW=60

# 老虎证券配置
TIGER_BASE_URL=https://www.itiger.com
TIGER_API_URL=https://api.itiger.com
TIGER_API_KEY=your_tiger_api_key
TIGER_RATE_LIMIT=100
TIGER_RATE_WINDOW=60

# TradingView 配置
TRADINGVIEW_BASE_URL=https://www.tradingview.com
TRADINGVIEW_API_URL=https://api.tradingview.com
TRADINGVIEW_RATE_LIMIT=100
TRADINGVIEW_RATE_WINDOW=60

# Redis 配置（可选）
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_PASSWORD=

# 数据库配置（可选）
DATABASE_URL=sqlite:///webagents.db

# API 服务配置
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4